{
  "platform": "github",
  "prHourlyLimit": 0,
  "baseBranches": [
    "main"
  ],
  "packageRules": [
    {
      "matchDatasources": ["docker"],
       "matchBaseBranches": ["master"],
      "matchPackageNames": ["n8nio/n8n"],
      "overrideDatasource": "github-releases",
      "overridePackageName": "n8n-io/n8n",
      "pinDigests": false
    }
  ],
  "extends": [
    "config:best-practices"
  ],
  "respectLatest": true,
  "ignoreUnstable": true
}
